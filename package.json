{
	"name": "animelens",
	"module": "src/index.ts",
	"type": "module",
	"version": "0.1.0-alpha",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development bun run src/index.ts",
		"start": "bun run src/index.ts",
		"build": "tsc",
		"format": "prettier --write .",
		"db:generate": "cross-env DATABASE_URL=./dev.db prisma generate",
		"db:migrate": "cross-env DATABASE_URL=./dev.db prisma migrate dev",
		"db:push": "cross-env DATABASE_URL=./dev.db prisma db push"
	},
	"devDependencies": {
		"@types/bun": "^1.2.21",
		"cross-env": "^10.0.0",
		"prettier": "^3.6.2"
	},
	"peerDependencies": {
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@discordx/pagination": "^4.4.0",
		"@prisma/client": "^6.15.0",
		"chalk": "^5.6.0",
		"consola": "^3.4.2",
		"discord.js": "^14.22.1",
		"prisma": "^6.15.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unloopedmido/animelens.git"
	},
	"license": "MIT",
	"author": "Mohammed Haism <mohdhaism16@gmail.com>"
}
